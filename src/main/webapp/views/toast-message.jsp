<div id="divToastMessage" class="toast red-color" role="alert" aria-live="assertive" aria-atomic="true" style="position: absolute; top: 0; right: 0;" data-delay="10000">
  <div class="toast-header">
    <img src="images/logo.png" class="rounded mr-2" alt="...">
			<strong class="mr-auto">Alert Message</strong>
    <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="toastBodyMessage" class="toast-body">${message}</div>
</div>